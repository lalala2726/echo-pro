<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zhangchuangla.system.mapper.SysLoginLogMapper">

    <resultMap id="BaseResultMap" type="cn.zhangchuangla.system.model.entity.SysLoginLog">
        <id property="id" column="id"/>
        <result property="username" column="username"/>
        <result property="status" column="status"/>
        <result property="ip" column="ip"/>
        <result property="address" column="address"/>
        <result property="browser" column="browser"/>
        <result property="os" column="os"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateBy" column="update_by"/>
        <result property="isDeleted" column="is_deleted"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <update id="cleanLoginLog">
        truncate table sys_login_log
    </update>
    <select id="listLoginLog" resultMap="BaseResultMap">
        select *
        from sys_login_log
    </select>

</mapper>
