<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zhangchuangla.system.mapper.SysDictItemMapper">

    <resultMap id="BaseResultMap" type="cn.zhangchuangla.system.model.entity.SysDictItem">
        <id property="id" column="id"/>
        <result property="dictCode" column="dict_code"/>
        <result property="value" column="value"/>
        <result property="label" column="label"/>
        <result property="tagType" column="tag_type"/>
        <result property="status" column="status"/>
        <result property="sort" column="sort"/>
        <result property="remark" column="remark"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
    </resultMap>
    <select id="listSpecifyDictData" resultMap="BaseResultMap">
        select *
        from sys_dict_item
        where dict_code = #{dictCode}
    </select>

    <select id="listDictData" resultMap="BaseResultMap">
        select *
        from sys_dict_item
    </select>
</mapper>
