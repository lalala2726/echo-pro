<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zhangchuangla.system.mapper.FileManagementMapper">

    <resultMap id="BaseResultMap" type="cn.zhangchuangla.system.model.entity.FileManagement">
        <id property="id" column="id"/>
        <result property="fileName" column="file_name"/>
        <result property="originalFileName" column="original_file_name"/>
        <result property="originalRelativeFileLocation" column="original_relative_file_location"/>
        <result property="previewRelativeFileLocation" column="preview_relative_file_location"/>
        <result property="fileUrl" column="file_url"/>
        <result property="previewImage" column="preview_image"/>
        <result property="fileSize" column="file_size"/>
        <result property="fileType" column="file_type"/>
        <result property="fileExtension" column="file_extension"/>
        <result property="storageType" column="storage_type"/>
        <result property="bucketName" column="bucket_name"/>
        <result property="md5" column="md5"/>
        <result property="uploaderId" column="uploader_id"/>
        <result property="uploaderName" column="uploader_name"/>
        <result property="uploadTime" column="upload_time"/>
        <result property="isDeleted" column="is_deleted"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateBy" column="update_by"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <select id="fileList" resultMap="BaseResultMap">
        select *
        from file_management
    </select>
</mapper>
